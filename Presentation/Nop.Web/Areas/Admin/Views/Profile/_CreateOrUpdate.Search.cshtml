@model ProfileModel

<div class="card-body">
    <div class="search-body">
        <div class="row">
            <div class="col-md-5">
                <div class="form-group row">
                    <div class="col-md-4">
                        <nop-label asp-for="SearchApplicantId" />
                    </div>
                    <div class="col-md-8">
                        <nop-editor asp-for="SearchApplicantId" />
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="text-center col-12">
                <button type="button" id="search-profile" class="btn btn-primary btn-search">
                    <i class="fas fa-magnifying-glass"></i>
                    @T("Admin.Common.Search")
                </button>
            </div>
        </div>
    </div>
</div>

<script>
    document.getElementById("search-profile").addEventListener("click", function () {
        var applicantId = document.getElementById("SearchApplicantId").value;
        window.location.href = '/Admin/Profile/Create?applicantId=' + encodeURIComponent(applicantId);
    });
</script>